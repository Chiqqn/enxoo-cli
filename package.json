{
  "name": "enxoo",
  "description": "enxoo sfdx cli extensions",
  "version": "0.1.21",
  "author": "Maciej Simm @maciejsimm-enxoo",
  "bugs": "https://github.com/maciejsimm/enxoo-cli/issues",
  "dependencies": {
    "@oclif/command": "1",
    "@oclif/config": "^1.14.0",
    "@oclif/errors": "1",
    "@salesforce/command": "^3.0.0",
    "@salesforce/core": "^2.3.0",
    "salesforce": "^0.1.2",
    "tslib": "^1.11.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/plugin-help": "^2.2.3",
    "@oclif/test": "^1",
    "@salesforce/dev-config": "^1.1.4",
    "@types/chai": "^4.2.11",
    "@types/mocha": "^5",
    "@types/node": "^10.17.19",
    "chai": "^4",
    "globby": "^8",
    "mocha": "^5",
    "nyc": "^13",
    "sinon": "5",
    "ts-node": "^7",
    "typescript": "^3.8.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/lib",
    "/messages",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/maciejsimm/enxoo-cli",
  "keywords": [
    "sfdx-plugin"
  ],
  "license": "MIT",
  "oclif": {
    "commands": "./lib/commands",
    "topics": {
      "hello": {
        "description": "Commands to say hello."
      }
    },
    "devPlugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "maciejsimm/enxoo-cli",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json npm-shrinkwrap.json",
    "posttest": "tslint -p test -t stylish",
    "prepare": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme && npm shrinkwrap",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  }
}
